{
  "version": 2,
  "builds": [
    {
      "src": "oneirvision/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd oneirvision && npm install && npm run build"
}
